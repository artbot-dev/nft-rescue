{
  "name": "nft-rescue",
  "version": "1.1.0",
  "description": "CLI tool to backup NFT assets stored on centralized/at-risk infrastructure",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "nft-rescue": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "vitest run tests/e2e",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "nft",
    "backup",
    "ethereum",
    "ipfs",
    "rescue",
    "web3",
    "decentralized"
  ],
  "author": "artbot-dev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artbot-dev/nft-rescue.git"
  },
  "homepage": "https://github.com/artbot-dev/nft-rescue#readme",
  "bugs": {
    "url": "https://github.com/artbot-dev/nft-rescue/issues"
  },
  "dependencies": {
    "alchemy-sdk": "^3.4.1",
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "commander": "^12.1.0",
    "ora": "^8.0.1",
    "viem": "^2.21.0"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/node": "^22.5.0",
    "@vitest/coverage-v8": "^2.0.0",
    "msw": "^2.4.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
